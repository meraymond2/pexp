{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6"],
    "module": "CommonJS",
    "declaration": true,
    "outDir": "out",
    "strict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "esModuleInterop": true,
    "rootDir": "src/",
    "downlevelIteration": true
  }
}
